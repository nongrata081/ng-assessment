{
	"name": "ng-assessment",
	"version": "0.5.0",
	"license": "MIT",
	"scripts": {
		"~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Custom dev flow ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~": "",
		"prettify": "pretty-quick",
		"prettify-staged": "pretty-quick --staged",
		"commit": "git-cz",
		"enforce:gitflow": "node ./node_modules/.bin/enforce-gitflow-branches",
		"recommended:bump": "conventional-recommended-bump --preset=angular",
		"generate:changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"compodoc:shipping": "npx compodoc -p apps/shipping/tsconfig.app.json -d compodoc/shipping",
		"compodoc:shipping-backend": "npx compodoc -p apps/shipping-backend/tsconfig.app.json -d compodoc/shipping-backend",
		"lighthouse:mobile": "node ./node_modules/.bin/lighthouse http://localhost:4000 --output-path=./lighthouse/mobile/report.html --view",
		"lighthouse:desktop": "node ./node_modules/.bin/lighthouse http://localhost:4000 --emulated-form-factor=none --output-path=./lighthouse/desktop/report.html --view",
		"backstop:reference": "./node_modules/backstopjs/cli/index.js reference",
		"backstop:test": "./node_modules/backstopjs/cli/index.js test && ./node_modules/backstopjs/cli/index.js openReport",
		"backstop:test:ci": "./node_modules/backstopjs/cli/index.js test",
		"backstop:approve": "yarn backstop approve",
		"backstop:open:latest": "yarn backstop openReport",
		"a11y:test": "./node_modules/pa11y/bin/pa11y.js --reporter html http://localhost:4200/ > a11y/pa11y-test-report.html",
		"~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Custom app scripts ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~": "",
		"fake:rest:api": "./node_modules/json-server/lib/cli/bin.js --watch fake-rest-api/endpoints.json",
		"shipping:hmr": "ng serve shipping --configuration hmr",
		"serve": "node apps/shipping/serve-script",
		"compile:server": "webpack --config apps/shipping-backend/webpack.server.config.js --progress --colors",
		"serve:ssr": "node dist/apps/shipping-backend/server",
		"build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
		"build:client-and-server-bundles": "ng build --prod && ng run shipping:server:production",
		"ssr": "npm run build:ssr && npm run serve:ssr",
		"build:shell": "ng run shipping:app-shell",
		"build:stats": "ng build shipping --stats-json",
		"analyze:bundle:es5": "webpack-bundle-analyzer dist/apps/shipping/stats-es5.json",
		"analyze:bundle:es2015": "webpack-bundle-analyzer dist/apps/shipping/stats-es2015.json",
		"test:coverage": "ng test --code-coverage",
		"~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ NX ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~": "",
		"ng": "ng",
		"start": "npm run fake:rest:api & ng serve --o",
		"build": "ng build",
		"test": "ng test --passWithNoTests",
		"lint": "nx lint && ng lint",
		"e2e": "ng e2e",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"update": "ng update @nrwl/workspace",
		"update:check": "ng update",
		"workspace-schematic": "nx workspace-schematic",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~": ""
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^8.0.2",
		"@angular/cdk": "^8.0.1",
		"@angular/common": "8.0.2",
		"@angular/compiler": "8.0.2",
		"@angular/core": "8.0.2",
		"@angular/flex-layout": "^8.0.0-beta.26",
		"@angular/forms": "8.0.2",
		"@angular/material": "^8.0.1",
		"@angular/platform-browser": "8.0.2",
		"@angular/platform-browser-dynamic": "8.0.2",
		"@angular/platform-server": "8.0.2",
		"@angular/router": "8.0.2",
		"@fortawesome/fontawesome-free": "^5.9.0",
		"@nestjs/common": "6.3.2",
		"@nestjs/core": "6.3.2",
		"@nestjs/ng-universal": "1.1.1",
		"@nestjs/platform-express": "6.3.2",
		"@nguniversal/express-engine": "7.1.1",
		"@nguniversal/module-map-ngfactory-loader": "7.1.1",
		"@ngx-translate/core": "11.0.1",
		"@ngx-translate/http-loader": "4.0.0",
		"@nrwl/angular": "8.1.2",
		"@types/big.js": "^4.0.5",
		"@types/chart.js": "^2.7.54",
		"angular-bootstrap-md": "^7.5.4",
		"big.js": "^5.2.2",
		"chart.js": "^2.5.0",
		"core-js": "2.6.9",
		"hammerjs": "^2.0.8",
		"reflect-metadata": "0.1.13",
		"rxjs": "6.4.0",
		"zone.js": "0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.800.4",
		"@angular/cli": "8.0.0",
		"@angular/compiler-cli": "8.0.2",
		"@angular/language-service": "8.0.2",
		"@angularclass/hmr": "2.1.3",
		"@babel/core": "^7.4.5",
		"@commitlint/cli": "latest",
		"@commitlint/config-conventional": "latest",
		"@commitlint/travis-cli": "^8.0.0",
		"@compodoc/compodoc": "^1.1.9",
		"@nestjs/schematics": "^6.3.0",
		"@nestjs/testing": "^6.2.4",
		"@nrwl/cypress": "8.1.2",
		"@nrwl/jest": "8.1.2",
		"@nrwl/nest": "8.1.2",
		"@nrwl/node": "8.1.2",
		"@nrwl/workspace": "8.1.2",
		"@rubedo/devkit": "0.1.2",
		"@storybook/addon-actions": "^5.1.9",
		"@storybook/addon-links": "^5.1.9",
		"@storybook/addon-notes": "^5.1.9",
		"@storybook/addons": "^5.1.9",
		"@storybook/angular": "^5.1.9",
		"@types/jest": "24.0.9",
		"@types/node": "8.9.5",
		"babel-loader": "^8.0.6",
		"backstopjs": "^4.1.9",
		"codelyzer": "5.0.1",
		"commitizen": "latest",
		"conventional-changelog-cli": "latest",
		"conventional-recommended-bump": "latest",
		"cypress": "3.3.1",
		"cz-conventional-changelog": "latest",
		"dotenv": "6.2.0",
		"enforce-gitflow-branches": "latest",
		"husky": "latest",
		"jest": "24.1.0",
		"jest-preset-angular": "7.1.1",
		"json-server": "0.17.4",
		"lighthouse": "^5.1.0",
		"nodemon": "1.19.1",
		"npm-run-all": "4.1.5",
		"pa11y": "^5.1.0",
		"pa11y-reporter-html": "^1.0.0",
		"prettier": "1.16.4",
		"pretty-quick": "1.11.1",
		"rimraf": "2.6.3",
		"string-replace-loader": "2.2.0",
		"ts-jest": "24.0.0",
		"ts-loader": "5.4.5",
		"ts-node": "7.0.1",
		"tslint": "5.11.0",
		"typescript": "3.4.5",
		"wait-on": "3.2.0",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "3.3.5"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "run-s prettify-staged lint",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-push": "run-s enforce:gitflow prettify lint test e2e"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
